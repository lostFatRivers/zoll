 * 
 * @author qianhang
 * 
 * @date 2015年8月29日 下午4:00:56
 * 
 * @project zoll-TowerDefense
 * 
 */
public class Point {
	private int coords_x;
	private int coords_y;

	public Point() {
		super();
	}

	public Point(int coords_x, int coords_y) {
		super();
		this.coords_x = coords_x;
		this.coords_y = coords_y;
	}

	public int getCoords_x() {
		return coords_x;
	}

	public void setCoords_x(int coords_x) {
		this.coords_x = coords_x;
	}

	public int getCoords_y() {
		return coords_y;
	}

	public void setCoords_y(int coords_y) {
		this.coords_y = coords_y;
	}

	@Override
	public String toString() {
		return "Point [coords_x=" + coords_x + ", coords_y=" + coords_y + "]";
	}
	
}
